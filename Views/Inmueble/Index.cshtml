@model IEnumerable<Inmobiliaria2.Models.Inmueble>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Inmueble
            </th>
            <th>
                Propietario
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Direccion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Disponible)
            </th>
 
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdInmueble)
            </td>
             <td>
                @Html.DisplayFor(modelItem => item.Propietario.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Direccion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Disponible)
            </td>

                <td>
                    @Html.ActionLink("Edit", "Edit", new { id=item.IdInmueble })
                    @Html.ActionLink("Details", "Details", new { id=item.IdInmueble })
                    @Html.ActionLink("Delete", "Delete", new { id=item.IdInmueble })
                </td>
        </tr>
}
    </tbody>
</table>
